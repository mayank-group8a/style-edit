{% if template contains 'cart' %}
<script type="text/javascript" defer>
setInterval(function() {
  // at cart page
    if ($(".Cart #dcodeInfo").length > 0) {
        var totD = $('span.dcode-total-value').text() / 4;
        console.log(totD);
        $('p.cart span').text(totD.toFixed(2)).prepend(' $').prepend(' of ');

        $('.QuantitySelector__Button').click(function() {
            var tot = $('span.dcode-total-value').text() / 4;
            $('p.cart span').text(tot.toFixed(2)).prepend(' $').prepend(' of ');
        });
    } else {
        var cartTotal = $('.Cart__Total span').text().replace('$', '');
        var cartInstallment = cartTotal / 4;
        $('p.cart span').text(cartInstallment.toFixed(2)).prepend(' $').prepend(' of ');
        $('.QuantitySelector__Button').click(function() {
            var cartTotal = $('.Cart__Total span').text().replace('$', '');
            var cartInstallment = cartTotal / 4;
            $('p.cart span').text(cartInstallment.toFixed(2)).prepend(' $').prepend(' of ');
        });
    }
}, 0);
</script> 
{% endif %}

<script type="text/javascript" defer>
setInterval(function() {
  // at index gift card

   var giftCard = $('span.ProductMeta__Price').text().replace('$', '')/4;
  $('p.index span').text(giftCard.toFixed(2)).prepend(' $').prepend(' of ');

  $('.HorizontalList__Item .SizeSwatch__Radio').on('change', function(){
      giftCard = $(this).parents('.FeaturedProduct__Info').find('span.ProductMeta__Price').text().replace('$', '')/4;
      $('p.index span').text(giftCard.toFixed(2)).prepend(' $').prepend(' of ');
  });
}, 0);
</script>
  
